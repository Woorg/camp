.video
	display block
	&__in
		position relative
		padding-top 18px
		padding-bottom 40px
		@extend .in
		background #FFF
	&__title
		margin-bottom 17px
		color #005393
		font-calibri()
		set-font-size(33px)
		font-weight 400
		line-height 33px
		line-height remify(33px)
		text-transform uppercase
		text-align center
	&__embed
		border 2px solid #0b79be
		margin auto
		width 708px
		height 400px
		img
			width 100%
			height 100%

